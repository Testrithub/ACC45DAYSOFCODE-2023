#include <stdio.h>

int main()
{
   int a=0,b=1,c,i,n;
   printf("enter a number");
   scanf("%d",&n);
   printf("%d%d\n",a,b);
   i=1;
   while(i<=n-2){
   
  
   c=a+b;
   printf("%d\t",c);
    a=b;
   b=c;
       
   }
   
   
                                                                                       

    return 0;
}
